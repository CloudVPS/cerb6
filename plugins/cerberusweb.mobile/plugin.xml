<!DOCTYPE plugin SYSTEM "../../libs/devblocks/plugin.dtd">
<plugin>
	<id>cerberusweb.mobile</id>
	<name>Cerberus Helpdesk Mobile UI</name>
	<description>Minimalistic interface for mobile devices.</description>
	<author>WebGroup Media, LLC.</author>
	<revision>0</revision>
	
	<mapping>
		<uri value="mobile" extension_id="cerberusweb.mobile.handler" />
	</mapping>
	
	<extensions>
	
		<extension point="cerberusweb.mobile.handler">
			<id>cerberusweb.mobile.handler</id>
			<name>Mobile Interface Module</name>
			<class>
				<file>classes.php</file>
				<name>MobileHandler</name>
			</class>
			<params>
			</params>
		</extension>
		
	</extensions>
</plugin>